Bundle-Name: Content Targeting API
Bundle-SymbolicName: com.liferay.content.targeting.api
Bundle-Version: 1.0.0
Export-Package:\
	com.liferay.content.targeting,\
	com.liferay.content.targeting.api.model,\
	com.liferay.content.targeting.lar,\
	com.liferay.content.targeting.model,\
	com.liferay.content.targeting.model.impl,\
	com.liferay.content.targeting.rule.categories,\
